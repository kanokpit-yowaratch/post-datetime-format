{
	"compilerOptions": {
		"target": "es2015",
		"lib": [
			"es2017",
			"es7",
			"es6",
			"dom"
		],
		"module": "commonjs",
		"declaration": true,
		"outDir": "dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": [
		"**/*"
	],
	"exclude": [
		"node_modules",
		"dist"
	],
}